<div class="row">
  <div class="col-xs-6">
    <h4>Appointment Type</h4>
    <ul class="list-group">
      <li class="list-group-item">
        {{model.type}}
      </li>
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-xs-6">
    <h4>Service Option</h4>
    <ul class="list-group">
      <li class="list-group-item">
        {{model.name}}
      </li>
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-xs-6">
    <h4>Clinic</h4>
    <div class="list-group">
      {{#each model.clinics as |clinic|}}
          {{#link-to 'clinic' clinic class="list-group-item"}}
            {{clinic.name}}
          {{/link-to}}
      {{/each}}
    </div>
  </div>
</div>
